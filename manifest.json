{
  "manifest_version": 2,

  "name": "Tab Meister",
  "description": "Extension de Google que separa tabs dada una palabra clave",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",

  "permissions": ["tabs"]
}
