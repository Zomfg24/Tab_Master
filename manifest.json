{
  "manifest_version": 2,

  "name": "Tab Meister",
  "description": "Extension de Google que separa tabs dada una palabra clave",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
